{
	"Angular Filter": {
		"prefix": "ngfilter",
		"description": "Angular 1.x Filter\n",
		"body": [
			"(function () {",
			"\t'use strict';",
			"",
			"\tangular",
			"\t\t.module('${Module}')",
			"\t\t.filter('${Filter}', ${Filter})",
			"",
			"\tfunction ${Filter}() {",
			"",
			"\t\tfunction ${Filter}Filter(${Params}){",
			"\t\t\t$0",
			"\t\t\treturn ${Params}",
			"\t\t}",
			"",
			"\t\treturn ${Filter}Filter;",
			"\t}",
			"",
			"}());"
		]
	}
}