{
	"Angular Run": {
		"prefix": "ngrun",
		"description": "Angular 1.x Run\n",
		"body": [
			"(function () {",
			"\t'use strict';",
			"",
			"\tangular",
			"\t\t.module('${Module}')",
			"\t\t.run(${Run}Run)",
			"",
			"\t${Run}Run.\\$inject = [];",
			"",
