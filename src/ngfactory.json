{
	"Angular Factory": {
		"prefix": "ngfactory",
		"description": "Angular 1.x Factory\n",
		"body": [
			"(function () {",
			"\t'use strict';",
			"",
			"\tangular",
			"\t\t.module('${Module}')",
			"\t\t.factory('${Factory}', ${Factory})",
			"",
			"\t${Factory}.\\$inject = ['\\$http'];",
			"",
			"\tfunction ${Factory}(\\$http) {",
			"\t\tvar service = {",
			"\t\t\t${getData}: ${getData}",
			"\t\t};",
			"",
			"\t\tfunction ${getData}() {",
			"\t\t\t$0",
			"\t\t}",
			"",
			"\t\treturn service;",
			"\t}",
			"})();"
		]
	}
}