            "  ng-show=\"!${1:formName}.${2:inputName}.\\$pristine && ${1:formName}.${2:inputName}.\\$dirty && ${1:formName}.${2:inputName}.\\$invalid\"",
            ">",
            "\t<p ng-message=\"${3:validationName}\">${4:message}</p>",
            "\t<p ng-message=\"${5:validationName, validationName}\">${6:message}</p>",
            "\t<p ng-message-default>${0:This field has an input error.}</p>",
